{
  "name": "webpublish",
  "version": "1.5.10",
  "author": "tangweitian <tstwt@qq.com>",
  "description": "一个快速发布文件夹、文件、MBTiles的工具",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "hostUpdateUrl": "http://oss.tangweitian.cn/update/webpublish/host/",
  "homepage": "http://webpublish.tangweitian.cn",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:publish": "npm run build:changelog && node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "release": "node scripts/release.js",
    "test": "node scripts/test.js",
    "changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s",
    "changelog1": "conventional-changelog -h"
  },
  "build": {
    "productName": "WebPublish",
    "appId": "com.ts.webpublish",
    "directories": {
      "output": "build"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://oss.tangweitian.cn/update/webpublish",
        "description": "新增批量添加任务，尝试一下吧",
        "updateInfos": [
          "升级 升级Electron核心版本到18",
          "重构 添加任务流程，支持多个任务同时添加"
        ]
      }
    ],
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "icons/icon.icns"
    },
    "win": {
      "icon": "icons/icon.ico"
    },
    "linux": {
      "category": "Network",
      "icon": "icons",
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "dependencies": {
    "@mapbox/mbtiles": "^0.12.1",
    "@mapbox/sphericalmercator": "^1.2.0",
    "@mapbox/vector-tile": "^1.3.1",
    "@panter/vue-i18next": "^0.15.2",
    "async-lock": "^1.3.0",
    "axios": "^0.26.0",
    "batch": "^0.6.1",
    "compressing": "^1.5.1",
    "ejs": "^3.1.7",
    "electron-is": "^3.0.0",
    "electron-log": "^4.3.4",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.5",
    "element-ui": "^2.15.5",
    "escape-html": "^1.0.3",
    "express": "^4.18.0",
    "http-shutdown": "^1.2.2",
    "i18next": "^20.4.0",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.30",
    "mkdirp": "^1.0.4",
    "pbf": "^3.2.1",
    "sqlite3": "^5.0.2",
    "vue": "^2.6.11",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.1.6",
    "vuex": "^3.3.0",
    "vuex-router-sync": "^5.0.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@electron/remote": "^2.0.8",
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.2",
    "conventional-changelog-cli": "^2.1.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^18.1.0",
    "electron-builder": "^20.19.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^3.2.7",
    "enquirer": "^2.3.6",
    "execa": "^0.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy": "^1.18.1",
    "mini-css-extract-plugin": "^0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.0",
    "sass-loader": "^7.0.3",
    "sass-resources-loader": "^2.2.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
